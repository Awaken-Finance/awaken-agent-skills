{
  "name": "@awaken-finance/agent-kit",
  "version": "1.2.2",
  "description": "AI Agent toolkit for Awaken DEX on aelf blockchain â€” swap tokens, manage liquidity, fetch K-line data. Supports MCP, CLI, and SDK usage.",
  "type": "module",
  "main": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./mcp": "./src/mcp/server.ts"
  },
  "bin": {
    "awaken-setup": "./bin/setup.js"
  },
  "files": [
    "index.ts",
    "src/",
    "lib/",
    "bin/",
    "cli-helpers.ts",
    "awaken_query_skill.ts",
    "awaken_trade_skill.ts",
    "awaken_kline_skill.ts",
    "openclaw.json",
    "mcp-config.example.json",
    "README.md",
    "README.zh-CN.md",
    "LICENSE"
  ],
  "scripts": {
    "setup": "bun run bin/setup.ts",
    "mcp": "bun run src/mcp/server.ts",
    "test": "bun test __tests__/",
    "test:unit:main": "bun test $(find __tests__/unit -name '*.test.ts' ! -name 'core-query.mocked.test.ts' ! -name 'core-trade.mocked.test.ts' | sort)",
    "test:unit:mocked": "bun test __tests__/unit/core-query.mocked.test.ts && bun test __tests__/unit/core-trade.mocked.test.ts",
    "test:unit": "bun run test:unit:main && bun run test:unit:mocked",
    "test:unit:main:coverage": "bun run test:unit:main --coverage --coverage-reporter=text --coverage-reporter=lcov --coverage-dir=coverage/main",
    "test:unit:mocked:query:coverage": "bun test __tests__/unit/core-query.mocked.test.ts --coverage --coverage-reporter=text --coverage-reporter=lcov --coverage-dir=coverage/mocked/query",
    "test:unit:mocked:trade:coverage": "bun test __tests__/unit/core-trade.mocked.test.ts --coverage --coverage-reporter=text --coverage-reporter=lcov --coverage-dir=coverage/mocked/trade",
    "test:unit:mocked:coverage": "bun run test:unit:mocked:query:coverage && bun run test:unit:mocked:trade:coverage",
    "coverage:merge:unit": "mkdir -p coverage && cat coverage/main/lcov.info coverage/mocked/query/lcov.info coverage/mocked/trade/lcov.info > coverage/lcov.info",
    "coverage:badge": "bun run bin/generate-coverage-badge.ts",
    "test:unit:coverage": "bun run test:unit:main:coverage && bun run test:unit:mocked:coverage && bun run coverage:merge:unit",
    "test:integration": "bun test __tests__/integration/",
    "test:e2e": "bun test __tests__/e2e/",
    "coverage:gate": "bun run scripts/coverage-gate.ts",
    "test:coverage:ci": "COVERAGE_MIN_LINES=85 COVERAGE_MIN_FUNCS=80 bun run test:unit:coverage && bun run coverage:gate",
    "build:openclaw": "bun run bin/generate-openclaw.ts",
    "build:openclaw:check": "bun run bin/generate-openclaw.ts --check",
    "deps:check": "bun run scripts/check-deps-baseline.ts"
  },
  "keywords": [
    "awaken",
    "aelf",
    "dex",
    "swap",
    "liquidity",
    "kline",
    "ai-agent",
    "mcp",
    "model-context-protocol",
    "langchain",
    "blockchain",
    "defi"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Awaken-Finance/awaken-agent-skills.git"
  },
  "homepage": "https://github.com/Awaken-Finance/awaken-agent-skills#readme",
  "bugs": {
    "url": "https://github.com/Awaken-Finance/awaken-agent-skills/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@portkey/aelf-signer": "1.0.1",
    "aelf-sdk": "^3.5.1-beta.0",
    "axios": "^1.7.0",
    "bignumber.js": "^9.1.2",
    "commander": "^12.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
